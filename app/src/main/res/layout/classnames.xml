        android:id="@+id/CBlockReset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="@string/reset"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/BBlockReset" />


    <TextView
        android:id="@+id/DBlockHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="@string/d_block"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/CBlockHeader" />

    <EditText
        android:id="@+id/DBlockName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="90dp"
        android:background="@color/backgroundgray"
        android:importantForAutofill="no"
        android:inputType="textAutoCorrect"
        android:text=""
        android:textColor="@color/white"
        app:layout_constraintLeft_toRightOf="@id/DBlockReset"
        app:layout_constraintRight_toLeftOf="@id/DBlockHeader"
        app:layout_constraintTop_toTopOf="@id/CBlockName"
        tools:ignore="LabelFor"
        tools:targetApi="o" />

    <Button
        android:id="@+id/DBlockReset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="@string/reset"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/CBlockReset" />


    <TextView
        android:id="@+id/EBlockHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="@string/e_block"
        android:textColor="@color/white"
        app:layout_constraintRight_toLeftOf="@id/ABlockName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/DBlockHeader" />

    <EditText
        android:id="@+id/EBlockName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="90dp"
        android:background="@color/backgroundgray"
        android:importantForAutofill="no"
        android:inputType="textAutoCorrect"
        android:text=""
        android:textColor="@color/white"
        app:layout_constraintLeft_toRightOf="@id/EBlockReset"
        app:layout_constraintRight_toLeftOf="@id/EBlockHeader"
        app:layout_constraintTop_toTopOf="@id/DBlockName"
        tools:ignore="LabelFor"
        tools:targetApi="o" />

    <Button
        android:id="@+id/EBlockReset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="@string/reset"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/DBlockReset" />


    <TextView
