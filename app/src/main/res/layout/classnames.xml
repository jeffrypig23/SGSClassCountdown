<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/black">

    <TextView
        android:id="@+id/ABlockHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/a_block"
        android:textColor="@color/white"
        app:layout_constraintRight_toLeftOf="@id/ABlockName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/ABlockName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="90dp"
        android:background="@color/backgroundgray"
        android:importantForAutofill="no"
        android:inputType="textAutoCorrect"
        android:text=""
        android:textColor="@color/white"
        app:layout_constraintLeft_toRightOf="@id/ABlockReset"
        app:layout_constraintRight_toLeftOf="@id/ABlockHeader"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="LabelFor"
        tools:targetApi="o" />

    <Button
        android:id="@+id/ABlockReset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/reset"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/BBlockHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="@string/b_block"
        android:textColor="@color/white"
        app:layout_constraintRight_toLeftOf="@id/BBlockName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/ABlockHeader" />

    <EditText
        android:id="@+id/BBlockName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="90dp"
        android:background="@color/backgroundgray"
        android:importantForAutofill="no"
        android:inputType="textAutoCorrect"
        android:text=""
        android:textColor="@color/white"
        app:layout_constraintLeft_toRightOf="@id/CBlockReset"
        app:layout_constraintRight_toLeftOf="@id/CBlockHeader"
        app:layout_constraintTop_toTopOf="@id/ABlockName"
        tools:ignore="LabelFor"
        tools:targetApi="o" />

    <Button
        android:id="@+id/BBlockReset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="@string/reset"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/ABlockReset" />


    <TextView
        android:id="@+id/CBlockHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="@string/c_block"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/BBlockHeader" />

    <EditText
        android:id="@+id/CBlockName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="90dp"
        android:background="@color/backgroundgray"
        android:importantForAutofill="no"
        android:inputType="textAutoCorrect"
        android:text=""
        android:textColor="@color/white"
        app:layout_constraintLeft_toRightOf="@id/CBlockReset"
        app:layout_constraintRight_toLeftOf="@id/CBlockHeader"
        app:layout_constraintTop_toTopOf="@id/BBlockName"
        tools:ignore="LabelFor"
        tools:targetApi="o" />

